{
  "manifest_version": 3,
  "name": "Blink To Type",
  "description": "Teclado Virtual controlado por piscadas",
  "version": "1.0.0",
  "icons": {
    "16": "icons/logo-16.png",
    "48": "icons/logo-48.png",
    "128": "icons/logo-128.png"
  },
  "action": {
    "default_title": "Blink To Type",
    "default_icon": {
      "16": "icons/logo-16.png",
      "48": "icons/logo-48.png",
      "128": "icons/logo-128.png"
    }
  },
  "permissions": [
  "scripting",
  "activeTab",
  "tabs",
  "storage",
  "webNavigation"
],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "ui/*",
        "src/*",
        "libs/*",
        "icons/*",
        "models/*"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}